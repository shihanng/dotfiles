{
  "mcpServers": {
    "vectorcode-mcp-server": {
      "args": [],
      "command": "${userHome}/.local/share/mise/shims/vectorcode-mcp-server",
      "disabled": false
    },
    "memory": {
      "args": [],
      "command": "${userHome}/.local/share/mise/shims/mcp-server-memory",
      "disabled": false,
      "env": {
        "MEMORY_FILE_PATH": "${userHome}/memory.json"
      }
    },
    "context7": {
      "args": [],
      "command": "${userHome}/.local/share/mise/shims/context7-mcp",
      "disabled": false
    },
    "serena": {
      "args": ["start-mcp-server"],
      "command": "${userHome}/.local/share/mise/shims/serena",
      "disabled": false
    },
    "astro_doc": {
      "url": "https://mcp.docs.astro.build/mcp",
      "disabled": false
    }
  }
}
