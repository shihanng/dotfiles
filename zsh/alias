# Alias
alias rm="rm -i"
alias cp="cp -i"
alias mv="mv -i"
alias mi="open -a mi.app"
alias skim="open -a skim.app"
alias xfig="xfig -spec -bg white -fg black -latexfonts"

gotest () {
    go test "${@}" | sed ''/PASS/s//$(printf "\033[32mPASS\033[0m")/'' | sed ''/FAIL/s//$(printf "\033[31mFAIL\033[0m")/''
}
